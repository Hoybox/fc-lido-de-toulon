{
  "name": "fclido-app",
  "version": "1.0.0",
  "description": "Frontend and backend for FC Lido application.",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch:css\" \"nodemon server.mjs\"",
    "watch:css": "tailwindcss -i ./src/input.css -o ./dist/index.css --watch",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/index.css --minify",
    "build": "npm run build:css && node build.mjs",
    "start": "node server.mjs",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@google/genai": "^1.28.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.2",
    "esbuild-plugin-copy": "^2.1.1",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3"
  }
}
